  "scripts": {
    "lint:js": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "lint:js:fix": "yarn run lint --fix",
    "lint:css": "stylelint \"app/**/*.{css,scss}\"",
    "lint:css:fix": "yarn run lint-css --fix",
    "lint": "yarn run lint:js && yarn run lint:css",
    "test": "jest",
    "test:watch": "yarn run test --watch --notify",
    "test:cover": "yarn run test --coverage",
    "test:func": "cucumber-js --require-module @babel/register",
    "test:func:local:chrome": "cucumber-js --require-module @babel/register --parallel 4 --world-parameters='{\"local\": 1, \"browser\": \"chrome\"}'",
    "test:func:local:firefox": "cucumber-js --require-module @babel/register --parallel 4 --world-parameters='{\"local\": 1, \"browser\": \"firefox\"}'",
    "test:func:local:safari": "cucumber-js --require-module @babel/register --parallel 4 --world-parameters='{\"local\": 1, \"browser\": \"safari\"}'",
    "test:func:visual:chrome": "cucumber-js --require-module @babel/register --world-parameters='{\"local\": 1, \"display\": 1, \"browser\": \"chrome\"}'",
    "test:func:visual:firefox": "cucumber-js --require-module @babel/register --world-parameters='{\"local\": 1, \"display\": 1, \"browser\": \"firefox\"}'",
    "test:func:visual:safari": "cucumber-js --require-module @babel/register --world-parameters='{\"local\": 1, \"display\": 1, \"browser\": \"safari\"}'",
    "test:func:sauce:chrome": "cucumber-js --require-module @babel/register --fail-fast --world-parameters='{\"browser\": \"chrome\"}'",
    "test:func:sauce:firefox": "cucumber-js --require-module @babel/register --fail-fast --world-parameters='{\"browser\": \"firefox\"}'",
    "test:func:sauce:ie": "cucumber-js --require-module @babel/register --fail-fast --world-parameters='{\"browser\": \"ie\"}'",
    "test:func:sauce:safari": "cucumber-js --require-module @babel/register --fail-fast --world-parameters='{ \"browser\": \"safari\"}'",
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "babel-node tools/run build",
    "build:stats": "yarn run build --release --analyze",
    "start": "babel-node tools/run start",
    "sequelize": "babel-node ./node_modules/sequelize-cli/lib/sequelize"
  }
}
